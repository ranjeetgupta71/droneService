<h1>Pricings</h1>

<div id="pricings">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Duration</th>
        <th>Price</th>
      </tr>
    </thead>

    <% @durations.each do |duration| %>
      <tr>
        <td><%= duration.time %></td>
        <td><%= duration.price %></td>
      </tr>
    <% end %>
  </table>
</div>
