<h1>Locations</h1>

<div id="locations">
  <table class="table table-striped">
    <thead>
      <tr>
        <th>Name</th>
        <th>State</th>
      </tr>
    </thead>

    <% @locations.each do |location| %>
      <tr>
        <td><%= location.name %></td>
        <td><%= location.state %></td>
      </tr>
    <% end %>
  </table>
</div>
